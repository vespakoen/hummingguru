{
  "extends": "airbnb",
  "rules": {
    "semi": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-alert": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "class-methods-use-this": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "navigator": true,
    "WebSocket": true
  }
}
